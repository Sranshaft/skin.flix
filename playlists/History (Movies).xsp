<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>History (Movies)</name>
    <match>10</match>
    <rule field="lastplayed" operator="inthelast">
        <value>7 days</value>
    </rule>
    <order direction="descending">lastplayed</order>
</smartplaylist>
